<Window x:Class="TurismoReal.Vistas.ListarCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TurismoReal.Vistas"
        mc:Ignorable="d"
        Title="ListarCliente" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="../Images/2.jpg" Opacity="0.6"/>
        </Grid.Background>
        <DataGrid Name ="GridCli" Height="154" Width="auto" ItemsSource="{Binding}" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,199,0,0" CanUserAddRows="False" CanUserDeleteRows="False" CanUserResizeRows="False"/>
        <Button Content="Volver" HorizontalAlignment="Left" Margin="668,76,0,0" VerticalAlignment="Top" Height="24" Width="65" Click="Button_Click"/>
    </Grid>
</Window>
